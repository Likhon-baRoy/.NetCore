<h1>Order something by pressing the button</h1>

<button class="button button-blue-black" type="button" id="button-load">Load Programming Languages</button>
<div class="box list-item-content"></div>
<script>
    document.querySelector("#button-load").addEventListener("click", async function () {
        var response = await fetch("programming-languages");
        var list = await response.text();
        document.querySelector(".list-item-content").innerHTML = list;
    });
</script>